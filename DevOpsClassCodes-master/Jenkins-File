pipeline {
    agent any
    tools{
        maven 'maven'
    }
    stages{
        stage("Checkout from git hub repo"){
            steps{
            git 'https://github.com/NikitasGithub/DevOpsClassCodes.git'    
            }
        }
         stage("code compile"){
            steps{
            bat "mvn compile"   
            }
        }
         stage("code test"){
            steps{
            bat "mvn test"   
            }
         } 
         stage("code quality test"){
            steps{
            bat "mvn pmd:pmd"   
            }
        }
         stage("code package"){
            steps{
            bat "mvn package"   
            }
        }
    }


}
